{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [140]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "ordered-imports": false,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "interface-name": false,
        "eofline": false,
        "object-literal-sort-keys": false,
        "prefer-const": false,
        "typedef": true,
        "class-name": true,
        "no-string-literal": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [140]
        }
    }
}